<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="289.0" prefWidth="319.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label id="Loginlbl" fx:id="welcomeText" text="Login">
      <font>
         <Font size="24.0" />
      </font></Label>
   <Label id="LoginErrorlbl" fx:id="loginErrorlbl" textFill="#ff0000">
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <TextField id="Usernametxt" fx:id="usernametxt" promptText="Username" />
   <PasswordField id="Passwordtxt" fx:id="passwordtxt" promptText="Password" />
    <Button id="LoginBtn" fx:id="loginBtn" onAction="#onLoginBtnClick" style="-fx-background-color: SkyBlue;" text="Login" underline="true">
      <font>
         <Font size="14.0" />
      </font></Button>
</VBox>
