<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="260.0" prefWidth="251.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Example.EndAssignment.Controller.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="initialPromtText" text="Enter username and password" />
   <Label fx:id="errorMessageLabel" />
   <TextField fx:id="usernameField" />
   <PasswordField fx:id="passwordField" />
    <Button fx:id="LogInBtn" onAction="#onLoginClick" text="Log in" />
</VBox>
